---
permalink: /sitemap.xml
sitemapIgnore: true
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/assets/styles/sitemap.xsl"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for item in collections.all %}
{%- if item.data.sitemapIgnore != true %}
  <url>
    <loc>https:/{{ item.url | url }}</loc>
    <changefreq>{% if item.data.changefreq %}{{ item.data.changefreq }}{% else %}weekly{% endif %}</changefreq>
    <priority>{% if item.data.priority %}{{ item.data.priority }}{% else %}0.5{% endif %}</priority>
  </url>
{% endif %}
{% endfor %}
</urlset>